@{
    ViewBag.Title = "Index";
}
@model IList<MvcEFTest.Models.User>

<table cellspacing="0" cellpadding="0" border="1">
    <tr>
        <td>UserID</td>
        <td>UserName</td>
        <td>Password</td>
        <td>IsEnable</td>
        <td>Actions</td>
    </tr>
    @foreach (var obj in Model) { 
        <tr>
            <td>@obj.UserID</td>
            <td>@obj.UserName</td>
            <td>@obj.Password</td>
            <td>@(obj.IsEnable==true?"启用":"禁用")</td>
            <td>@Html.ActionLink("编辑", "Edit", new { id = obj.UserID })</td>
        </tr>
    }
</table>
